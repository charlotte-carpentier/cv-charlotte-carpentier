{
  "globalStyle": "bg-[theme(--ma-nautic-blue)] shadow-md",
  "containerStyle": "container mx-auto px-4 py-4",
  "navStyle": "flex items-center justify-between px-12 h-16 gap-8",
  "menuContainerStyle": "menu-burger",
  "variants": {
    "toggleButton": {
      "default": "toggle-button lg:hidden",
      "icon": "burger_solid",
      "iconStyle": "text-[theme(--color-primary)]"
    },
    "dropdownMenu": {
      "default": "dropdown-menu absolute -top-full left-0 max-lg:bg-[theme(--color-light-bg)] w-full py-2 font-[theme(--font-display)] lg:static lg:flex-row lg:justify-end"
    }
  },
  "headers": [
    {
      "name": "main",
      "logo": "logo_full",
      "navigation": "main"
    },
    {
      "name": "alternate",
      "logo": "logo_minimal",
      "navigation": "alternate"
    }
  ]
}