{
  "globalStyle": "bg-[theme(--ma-nautic-blue)] shadow-md",
  "containerStyle": "container mx-auto px-4 py-4 flex justify-between items-center h-24", 
  "logoContainerStyle": "relative z-30 -mb-24",
  "logoSize": "w-32 md:w-48 h-32 md:h-48",
  "navContainerStyle": "flex-1 flex justify-end items-center h-full", 
  "navStyle": "flex items-center gap-8 h-full", 
  "menuContainerStyle": "menu-burger h-full flex items-center",
  "variants": {
    "toggleButton": {
      "default": "toggle-button lg:hidden flex items-center justify-center",
      "icon": "wheel",
      "iconStyle": "text-[theme(--ma-nautic-white)]",
      "containerSize": "w-8 h-8",
      "iconSize": "32"
    },
    "dropdownMenu": {
      "default": "dropdown-menu absolute -top-full left-0 max-lg:bg-[theme(--ma-nautic-blue)] w-full py-2 transition-all duration-500 ease-in-out font-[theme(--font-display)] lg:static lg:flex-row lg:justify-end z-20"
    }
  },
  "headers": [
    {
      "name": "main",
      "logo": "ma_logo",
      "navigation": "main"
    },
    {
      "name": "alternate",
      "logo": "ma_logo_mini",
      "navigation": "alternate"
    }
  ]
}