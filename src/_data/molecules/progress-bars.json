{
  "globalStyle": "bg-gray-200 rounded-md overflow-hidden",
  "barStyle": "h-3 transition-all duration-300 ease-in-out",
  "labelStyle": "text-md font-medium text-gray-700 mb-1",
  "valuesStyle": "flex justify-between text-xs text-gray-500 mt-1",
  "variants": {
    "default": "bg-gray-500",
    "primary": "bg-blue-500",
    "success": "bg-green-500",
    "warning": "bg-yellow-500",
    "danger": "bg-red-500"
  },
  "sizes": {
    "xs": "w-[150px]",
    "sm": "w-[200px]",
    "md": "w-[300px]",
    "lg": "w-[400px]",
    "xl": "w-[500px]",
    "full": "w-full"
  },
  "percentages": {
    "0": "w-0",
    "25": "w-1/4",
    "50": "w-1/2",
    "75": "w-3/4",
    "100": "w-full"
  },
  "progress-bars": [
    {
      "name": "default-progress",
      "percentage": "50",
      "style": "default",
      "size": "md",
      "showLabel": true,
      "heading": "progress_bar",
      "showValues": true,
      "minValue": "0",
      "maxValue": "100%"
    },
    {
      "name": "primary-progress",
      "percentage": "75",
      "style": "primary",
      "size": "md",
      "showLabel": true,
      "heading": "progress_bar",
      "showValues": true,
      "minValue": "0",
      "maxValue": "100%"
    },
    {
      "name": "success-progress",
      "percentage": "100",
      "style": "success",
      "size": "md",
      "showLabel": true,
      "heading": "progress_bar",
      "showValues": true,
      "minValue": "0",
      "maxValue": "100%"
    },
    {
      "name": "warning-progress",
      "percentage": "25",
      "style": "warning",
      "size": "md",
      "showLabel": true,
      "heading": "progress_bar",
      "showValues": true,
      "minValue": "0",
      "maxValue": "100%"
    },
    {
      "name": "danger-progress",
      "percentage": "0",
      "style": "danger",
      "size": "md",
      "showLabel": true,
      "heading": "progress_bar",
      "showValues": true,
      "minValue": "0",
      "maxValue": "100%"
    }
  ]
}